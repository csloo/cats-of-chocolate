import turtle
ourScreen = turtle.Screen()
ourTurtle = turtle.Turtle()



ourTurtle.color("brown")
ourTurtle.color("blue")
ourTurtle.color("red")
ourTurtle.color("yellow")
ourTurtle.color("purple")
ourTurtle.color("pink")
ourTurtle.color("black")
ourTurtle.color("brown")
ourTurtle.color("blue")
ourTurtle.color("red")
ourTurtle.color("yellow")
ourTurtle.color("purple")
ourTurtle.color("pink")
ourTurtle.color("black")
ourTurtle.pencolor("orange")
ourTurtle.fd(100)
ourTurtle.rt(90)
ourTurtle.fd(100)
ourTurtle.rt(90)
ourTurtle.fd(100)
ourTurtle.reset()
ourTurtle.pencolor("orange")
ourTurtle.color("green")
ourTurtle.fd(10)
ourTurtle.rt(90)
ourTurtle.fd(10)
ourTurtle.rt(90)
ourTurtle.fd(10)
ourTurtle.lt(90)
ourTurtle.fd(10)
ourTurtle.lt(90)
ourTurtle.fd(10)
ourTurtle.lt(90)
ourTurtle.fd(10)
ourTurtle.rt(90)
ourScreen.Title("awesomeness")


